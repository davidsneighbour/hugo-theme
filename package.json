{
  "name": "@davidsneighbour/hugo-theme",
  "description": "@davidsneighbour's GoHugo theme.",
  "version": "0.0.80",
  "license": "MIT",
  "private": true,
  "repository": "davidsneighbour/hugo-theme",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com> (https://davids-neighbour.com)",
  "bugs": "https://github.com/davidsneighbour/hugo-theme/issues",
  "homepage": "https://github.com/davidsneighbour/hugo-theme#readme",
  "devDependencies": {
    "@davidsneighbour/babel-config": "^5.2.2",
    "@davidsneighbour/bootstrap-config": "5.1.39",
    "@davidsneighbour/browserslist-config": "^5.2.4",
    "@davidsneighbour/commitlint-config": "^5.2.8",
    "@davidsneighbour/cssnano-config": "^5.2.10",
    "@davidsneighbour/cypress-config": "^5.2.5",
    "@davidsneighbour/eslint-config": "^5.2.5",
    "@davidsneighbour/htmlvalidate-config": "^5.2.4",
    "@davidsneighbour/markdownlint-config": "^5.2.2",
    "@davidsneighbour/postcss-config": "^5.2.5",
    "@davidsneighbour/prettier-config": "5.1.31",
    "@davidsneighbour/remark-config": "5.1.38",
    "@davidsneighbour/standard-version-config": "5.0.16",
    "@davidsneighbour/stylelint-config": "^5.2.5",
    "@davidsneighbour/textlint-config": "5.0.16",
    "@davidsneighbour/tools": "^5.2.9",
    "@davidsneighbour/webpack-config": "^5.2.7",
    "all-contributors-cli": "6.26.0"
  },
  "scripts": {
    "clean": "run-p clean:**",
    "clean:hugo": "rimraf public resources .hugo_build.lock hugo.log",
    "clean:node": "rimraf node_modules package-lock.json",
    "release": "standard-version --release-as patch -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release/postrelease",
    "release:minor": "standard-version --release-as minor -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release/postrelease"
  },
  "dependencies": {
    "cssnano": "^6.0.1"
  }
}
